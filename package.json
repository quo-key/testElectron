{
  "name": "testdemo",
  "version": "1.0.3",
  "description": "TestDemo - 将静态页面打包为桌面应用",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "npm run generate-icon && electron-builder --win nsis",
    "dist:all": "npm run client:build && npm run dist",
    "build:all": "npm run client:build && npm run generate-icon && electron-builder --win nsis",
    "dist:fast": "npm run generate-icon && electron-builder --win portable --x64 --dir",
    "quick-pack": "node ./scripts/quick-pack.js",
    "generate-icon": "node ./scripts/generate-icon.js",
    "upload-server": "node server/upload-server.js",
    "client:dev": "vite",
    "client:build": "vite build",
    "client:preview": "vite preview --port 5173",
    "dev": "npm run client:dev",
    "build": "npm run client:build",
    "fix:dist": "node ./scripts/remove-crossorigin.js",
    "package:win": "npm run client:build && npm run fix:dist && node ./scripts/copy-preload.js && npm run generate-icon && electron-builder --win nsis"
  },
  "author": "",
  "license": "MIT",
  "build": {
    "appId": "com.example.testdemo",
    "productName": "my-tools",
    "files": [
      "dist/**/*",
      "main.js",
      "preload.js",
      "package.json",
      "build/icon.ico",
      "test.html"
    ],
    "directories": {
      "buildResources": "build"
    },
    "icon": "build/icon.ico",
    "npmRebuild": false,
    "asar": true,
    "compression": "maximum",
    "artifactName": "${productName}-${version}.${ext}",
    "win": {
      "target": [
        "nsis-web",
        "nsis"
      ]
    },
    "forceCodeSigning": false
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "antd": "^5.7.0",
    "react-router-dom": "^6.11.2",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.8",
    "@vitejs/plugin-react": "^4.0.0",
    "electron": "^39.1.1",
    "electron-builder": "^26.0.12",
    "electron-packager": "^17.1.2",
    "jimp": "^1.6.0",
    "png-to-ico": "^3.0.1",
    "pureimage": "^0.4.18",
    "typescript": "^5.2.0",
    "vite": "^5.0.0"
  }
}